<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :phone_no %><br />
    <%= text_field_tag :phone_no, params[:phone_no] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>
